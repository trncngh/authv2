model User {
    id          String      @id @unique @default(cuid())
    email       String      @unique
    isActivated Boolean     @default(false)
    credential  Credential?
    role        Role?       @relation(fields: [roleId], references: [id])
    roleId      Int?
    updateAt    DateTime    @updatedAt
    createdAt   DateTime    @default(now())
}
